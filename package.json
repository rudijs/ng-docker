  "main": "index.js",
  "scripts": {
    "typings": "typings install",
    "test": "npm run karma:ci",
    "karma": "NODE_ENV=test karma start karma.conf.js",
    "karma:single": "npm run karma -- --single-run",
    "karma:ci": "npm run karma -- --single-run --browsers PhantomJS",
    "karma:cov": "browser-sync start --server test/coverage/ --directory --files \"test/coverage/**/*\"",
    "lint": "tslint src/**/*.ts",
    "lint:watch": "chokidar 'src/**/*.ts' -c 'npm run lint'",
    "build": "rm -rfv build && webpack",
    "build:watch": "npm run build -- --watch",
    "build:prod": "npm run dist-clean && NODE_ENV=production npm run build && mv -v dist/index.html httpd && npm shrinkwrap",
    "build:prod:cdn": "CDN=true npm run build:prod",
    "dist-clean": "rm -rfv dist/",
    "start": "webpack-dev-server --history-api-fallback --inline --hot",
    "start:dev": "browser-sync start --config bs-config.js",
    "start:prod": "NODE_ENV=production node server.js"
  },
